# -*- Makefile -*-

SRCS	=	push_swap.c \
			main.c		\
			parsing.c	\
			swap.c		\
			push.c		\
			reverse.c	\
			rotate.c	\
			utils_0.c	\
			utils_1.c	\
			utils_2.c	\

NAME	=	../push_swap
CC		=	cc
CFLAGS	=	-Wall -Wextra -Werror -I
INC_DIR	=	../includes
LIBFT	=	../libft.a

all:
	@$(CC) $(CFLAGS) $(INC_DIR) $(SRCS) $(LIBFT) -o $(NAME) -g
	@echo Mandatory is compiled.

